<!-- /.box -->
<div class="box">
	<div class="box-header bg-light">
        <h3>User's Claims</h3>
	</div>
	<div class="box-body">
        <div class="table-responsive">
            <table class="dataTable table table-bordered table-striped display nowrap margin-top-10 w-p100">
                <thead>
                    <tr>
                        <th>Claim</th>
                        <th>Value</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var claim in User.Claims)
                    {
                        <tr>
                            <td>@claim.Type</td>
                            <td>@claim.Value</td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
	<!-- /.box-body -->
</div>
<!-- /.box --> 