@if (TempData.ContainsKey("ErrorMessage"))
{
    foreach (var value in TempData.Values)
    {
        <script>
            ToastrMessage("@value?.ToString()", "Error");
        </script>
    }
    TempData.Remove("ErrorMessage");
}

